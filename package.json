{
  "name": "nestjs-auth-kit",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "tsc --project tsconfig.test.json && jest --config test/unit/jest-unit.json",
    "test:watch": "jest --watch --config test/unit/jest-unit.json",
    "test:cov": "jest --coverage --config test/unit/jest-unit.json",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --config test/unit/jest-unit.json",
    "test:e2e": "docker compose -f docker-compose.test.yml up -d test-db && tsc --project tsconfig.test.json && NODE_ENV=test jest --config test-dist/test/e2e/jest-e2e.config.js --maxWorkers=1 && docker compose down test-db",
    "prepare": "ts-patch install && typia patch"
  },
  "dependencies": {
    "@nestia/core": "^3.19.1",
    "@nestia/e2e": "^0.7.0",
    "@nestia/fetcher": "^3.19.1",
    "@nestia/sdk": "^3.19.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mongoose": "^10.1.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^8.0.7",
    "@nestjs/typeorm": "^10.0.2",
    "bcrypt": "^5.1.1",
    "cls-hooked": "^4.2.2",
    "cookie-parser": "^1.4.7",
    "dayjs": "^1.11.13",
    "google-auth-library": "^9.15.0",
    "joi": "^17.13.3",
    "mongoose": "^8.8.2",
    "mysql2": "^3.11.4",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "typia": "^6.12.0"
  },
  "devDependencies": {
    "@nestia/benchmark": "^0.2.3",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cls-hooked": "^4.3.9",
    "@types/cookie-parser": "^1",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "jest": "^29.5.0",
    "nestia": "^6.2.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.2.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "~5.6.3"
  },
  "packageManager": "yarn@4.5.3"
}
